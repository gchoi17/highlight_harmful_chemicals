{
    "name": "Harmful ingredient highlighter",
    "version": "1.0",
    "description": "Highlight harmful ingredients in Makeup and Skincare Products",
    "permissions": ["tabs"],
    "browser_action": {
        "default_popup": "popup.html"
        },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],
    "manifest_version": 2
}